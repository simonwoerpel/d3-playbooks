    "rules": {
      "semi": 2
    }
  },
  "homepage": "https://simonwoerpel.github.io/d3-playbooks/",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.26.1",
    "d3": "^4.4.0",
    "eslint": "^3.12.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-es6-recommended": "^0.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.17.0",
    "immutable": "^3.8.1",
    "node-sass": "^3.13.0",
    "promise-polyfill": "^6.0.2",
    "rollup": "^0.26.3",
    "rollup-plugin-node-resolve": "^1.5.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0"
  }
}
