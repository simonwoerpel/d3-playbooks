{
  "name": "d3-playbooks",
  "version": "0.0.1",
  "description": "A simple wrapper around d3v4, extended with some stuff",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors"
  },
  "author": "@simonwoerpel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonwoerpel/d3-playbooks"
  },
  "bugs": {
    "url": "https://github.com/simonwoerpel/d3-playbooks/issues"
  },
  "keywords": [
    "d3",
    "dataviz",
    "frontend",
    "javascript",
    "webdev"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
